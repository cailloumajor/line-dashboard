{
  "name": "factory-frontend",
  "version": "2.4.3",
  "description": "Web frontend for factory needs",
  "productName": "Factory Frontend",
  "author": "Arnaud Rocher <arnaud.roche3@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "tsc --noEmit --skipLibCheck && eslint --ext .js,.json,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "serve:e2e": "quasar build --debug && node integration/e2e-tests-server.mjs",
    "test:e2e": "start-test serve:e2e http-get://localhost:9090 \"cypress open --e2e\"",
    "test:e2e:ci": "start-test serve:e2e http-get://localhost:9090 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test TZ=UTC cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test TZ=UTC cypress run --component"
  },
  "dependencies": {
    "@cailloumajor/frontend-utils-wasm": "3.0.2",
    "@quasar/extras": "1.16.15",
    "@vueuse/core": "10.11.1",
    "centrifuge": "5.2.2",
    "locale-matcher": "2.0.0",
    "mande": "2.0.9",
    "pinia": "2.2.6",
    "quasar": "2.17.7",
    "vue": "3.5.13",
    "vue-i18n": "9.14.2",
    "vue-router": "4.4.5",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "3.1.0",
    "@intlify/vite-plugin-vue-i18n": "7.0.0",
    "@pinia/testing": "0.1.7",
    "@quasar/app-vite": "1.6.2",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "6.1.0",
    "@rollup/plugin-replace": "6.0.2",
    "@rushstack/eslint-patch": "1.10.5",
    "@types/connect-history-api-fallback": "1.5.4",
    "@types/express": "5.0.0",
    "@types/http-proxy": "1.17.15",
    "@types/node": "20.17.13",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "autoprefixer": "10.4.20",
    "colortranslator": "4.1.0",
    "connect-history-api-fallback": "2.0.0",
    "cypress": "13.1.0",
    "dotenv": "16.4.7",
    "eslint": "8.57.1",
    "eslint-plugin-cypress": "4.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-vue": "9.31.0",
    "events": "^3.3.0",
    "express": "4.21.2",
    "express-static-gzip": "2.2.0",
    "http-proxy": "1.18.1",
    "msw": "2.6.5",
    "prettier": "3.3.3",
    "prettier-plugin-jsdoc": "1.3.2",
    "start-server-and-test": "2.0.10",
    "typescript": "5.2.2",
    "vite-plugin-compression": "0.5.1"
  },
  "engines": {
    "node": "^20.0.0",
    "npm": ">= 6.13.4",
    "yarn": "^1.21.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
